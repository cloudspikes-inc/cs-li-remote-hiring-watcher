{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SecretsManagerFullAccessForProjects",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:*"
      ],
      "Resource": [
        "arn:aws:secretsmanager:*:*:secret:cs-li-remote-hiring-watcher-*",
        "arn:aws:secretsmanager:*:*:secret:cs-linkedin-scanner-*"
      ]
    },
    {
      "Sid": "KmsFullAccess",
      "Effect": "Allow",
      "Action": [
        "kms:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "EC2FullAccess",
      "Effect": "Allow",
      "Action": [
        "ec2:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "S3FullAccess",
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "RDSFullAccess",
      "Effect": "Allow",
      "Action": [
        "rds:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "ECRFullAccess",
      "Effect": "Allow",
      "Action": [
        "ecr:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "DynamoDBFullAccess",
      "Effect": "Allow",
      "Action": [
        "dynamodb:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "CloudWatchFullAccess",
      "Effect": "Allow",
      "Action": [
        "cloudwatch:*",
        "logs:*",
        "events:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "TaggingFullAccess",
      "Effect": "Allow",
      "Action": [
        "tag:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "BillingAndCostExplorerAccess",
      "Effect": "Allow",
      "Action": [
        "aws-portal:ViewBilling",
        "aws-portal:ViewUsage",
        "aws-portal:ViewAccount",
        "aws-portal:ModifyAccount",
        "ce:GetCostAndUsage",
        "ce:GetCostForecast",
        "ce:GetUsageForecast",
        "ce:DescribeReport",
        "ce:ListCostCategoryDefinitions",
        "ce:GetAnomalyMonitors",
        "ce:GetAnomalySubscriptions",
        "ce:GetAnomalies",
        "cur:DescribeReportDefinitions",
        "cur:PutReportDefinition",
        "cur:DeleteReportDefinition",
        "cur:ModifyReportDefinition"
      ],
      "Resource": "*"
    },
    {
      "Sid": "AmazonQDeveloperAccess",
      "Effect": "Allow",
      "Action": [
        "q:*",
        "iam:AttachUserPolicy",
        "iam:AttachRolePolicy",
        "iam:ListAttachedUserPolicies",
        "iam:ListAttachedRolePolicies",
        "iam:GetPolicy",
        "iam:GetPolicyVersion"
      ],
      "Resource": [
        "arn:aws:iam::aws:policy/AmazonQDeveloperAccess",
        "*"
      ]
    },
    {
      "Sid": "ResourceExplorerFullAdminAccess",
      "Effect": "Allow",
      "Action": [
        "resource-explorer-2:*"
      ],
      "Resource": "*"
    }
  ]
}
